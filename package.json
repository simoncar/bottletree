    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/.expo/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "./__mocks__/mockFirebase",
      "./jest-setup.js"
    ]
