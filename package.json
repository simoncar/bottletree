    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "predeploy": "expo export -p web",
    "deploy-hosting": "npm run predeploy && firebase deploy --only hosting"
