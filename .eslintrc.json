{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
		"expo/tsconfig.base",
        "standard",
        "plugin:react/recommended",
		"@react-native-community",
		 "eslint-config-prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
		"react/prop-types": "off",
		"react/display-name": "off",
		"react-native/no-inline-styles": 2,
		"react-native/no-unused-styles": 2
	
	
    }
}
