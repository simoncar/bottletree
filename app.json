{
  "expo": {
    "name": "One Build",
    "slug": "bottletree",
    "version": "1.0.50",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bottletree",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9D96C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.simoncar.bottletree",
      "buildNumber": "51",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F9D96C"
      },
      "versionCode": 4,
      "package": "com.simoncar.bottletree"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c4340333-f23a-41cb-a647-97dee3bc0a01"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c4340333-f23a-41cb-a647-97dee3bc0a01"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to share the images you select with the project team."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/hammer.wav",
            "./assets/sounds/drill.wav"
          ]
        }
      ],
      "sentry-expo",
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "react-native-compressor"
    ]
  }
}
